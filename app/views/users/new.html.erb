<% form_for @user do |f| %>
    <%= f.error_messages  %>
  <p>
    <%= f.label :name %>
    <%= f.text_field :name %>
  </p>
  <p>
    <%= f.label :email %>
    <%= f.text_field :email %>
  </p>
  <p>
    <%= f.label :password %>
    <%= f.password_field :password %>
  </p>
  
  <%= f.submit "Create" %>
<% end %>

